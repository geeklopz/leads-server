<html>

<link href="css/bootstrap.min.css" rel="stylesheet">
<link href="css/leadslista.css" rel="stylesheet">
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js"></script>

<body style="padding:5%">

    <canvas class="line-chart"> </canvas>

    <div class="panel panel-default">
        <div class="panel-heading">Leads capturados pelo aplicativo</div>
        <table class="table">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>Email</th>
                    <th>Idade</th>
                    <th>FP</th>
                    <th>eSocial</th>
                </tr>
            </thead>
            <tbody>
                <%for(var i=0; i < lista.length; i++) {%>
                    <tr>
                        <td>
                            <%= lista[i].nome %>
                        </td>
                        <td>
                            <%= lista[i].email %>
                        </td>
                        <td class="itemListaValor">
                            <%= lista[i].idade %>
                        </td>
                        <td>
                            <%= lista[i].folhaPagamento %>
                        </td>
                        <td class=" itemListaIdade">
                            <%= lista[i].eSocial %>
                        </td>
                    </tr>
                    <%}%>
            </tbody>
        </table>
    </div>
</body>

<script src="js/leads/lista.js"></script>

</html>